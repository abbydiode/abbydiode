Options -Indexes

RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.html [NC,L]

#RewriteRule ^(.*)/$ /$1 [L,R]

# If domain requesting info is not glxy.dev or glxy.fyi then refer them to glxy.dev
RewriteCond %{HTTP_HOST} ^(w{3}\.)?sbox\.dev$ [NC]
RewriteRule ^(.*)$ "https://glxy.dev/sbox/$1" [P]

# Redirect Specific subdomains to IP:PORT
RewriteCond %{HTTP_HOST} ^demos\.glxi\.me$ [NC]
RewriteRule ^(.*)$ "http://server.glxi.me:6969/" [R=301,L]

# Force HTTPS
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ "https://%{HTTP_HOST}%{REQUEST_URI}" [R=301,L]

# If domain requesting info is not glxy.dev or glxy.fyi then refer them to glxy.dev
RewriteCond %{HTTP_HOST} !^(w{3}\.)?glxy\.dev$|^w*\.?glxy\.fyi$ [NC]
RewriteRule ^(.*)$ "https://glxy.dev/$1" [R=301,L]




